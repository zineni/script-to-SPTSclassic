        statRows.pp.startLabel.Text = "Start: " .. startStats.PsychicPower
        statRows.jf.startLabel.Text = "Start: " .. startStats.JumpForce
        statRows.ms.startLabel.Text = "Start: " .. startStats.MovementSpeed
        statRows.bt.startLabel.Text = "Start: " .. startStats.BodyToughness
        
        -- Reset end values and differences
        for _, row in pairs(statRows) do
            row.endLabel.Text = "End: N/A"
            row.diffLabel.Text = "Difference: -"
            row.diffLabel.TextColor3 = Color3.new(1, 1, 1)
        end
        
        -- Start time update
        if timeConnection then
            timeConnection:Disconnect()
        end
        timeConnection = RunService.Heartbeat:Connect(updateTime)
    end

    -- Stop tracking function
    local function stopTracking()
        if not trackingActive then return end
        
        endStats = getAllStatsText()
        trackingActive = false
        
        StatusLabel.Text = "Status: Completed"
        StatusLabel.TextColor3 = Color3.new(1, 1, 0.5)
        
        -- Update end values
        statRows.fs.endLabel.Text = "End: " .. endStats.FistStrength
        statRows.pp.endLabel.Text = "End: " .. endStats.PsychicPower
        statRows.jf.endLabel.Text = "End: " .. endStats.JumpForce
        statRows.ms.endLabel.Text = "End: " .. endStats.MovementSpeed
        statRows.bt.endLabel.Text = "End: " .. endStats.BodyToughness
                
        -- Update differences with colors
        local fsDiff, fsColor = formatDifference(startStats.FistStrength, endStats.FistStrength)
        local ppDiff, ppColor = formatDifference(startStats.PsychicPower, endStats.PsychicPower)
        local jfDiff, jfColor = formatDifference(startStats.JumpForce, endStats.JumpForce)
        local msDiff, msColor = formatDifference(startStats.MovementSpeed, endStats.MovementSpeed)
        local btDiff, btColor = formatDifference(startStats.BodyToughness, endStats.BodyToughness)
        
        statRows.fs.diffLabel.Text = "Difference: " .. fsDiff
        statRows.fs.diffLabel.TextColor3 = fsColor

