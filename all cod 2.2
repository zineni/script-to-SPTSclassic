-- ==============================
local function createESPSystem(commandConsole)
    local Players = game:GetService("Players")
    local RunService = game:GetService("RunService")
    local TweenService = game:GetService("TweenService")
    local plr = Players.LocalPlayer
    
    -- Local function for output messages (safe)
    local function showOutput(text)
        if commandConsole and commandConsole.showOutput then
            commandConsole.showOutput(text)
        else
            -- Fallback output to console
            print("[ESP]: " .. tostring(text))
        end
    end
    
    -- Tables for storing ESP elements
    local espFolders = {}
    local espConnections = {}
    local espEnabled = false
    
    -- Create GUI for ESP
    local ESPGui = Instance.new("ScreenGui")
    ESPGui.Name = "ESPGui"
    ESPGui.Parent = plr.PlayerGui
    ESPGui.ResetOnSpawn = false
    
    -- Safe teleport function
    local function safeTeleportTo(cf)
        local char = plr.Character
        if not char then return false end
        
        local hrp = char:FindFirstChild("HumanoidRootPart")
        if not hrp then return false end
        
        local success = pcall(function()
            hrp.CFrame = cf
        end)
        
        return success
    end
    
    -- Teleport to player function
    local function teleportToPlayer(targetPlayer)
        local targetChar = targetPlayer.Character
        if not targetChar then
            showOutput("âŒ Player " .. targetPlayer.Name .. " has no character")
            return
        end
        
        local targetHrp = targetChar:FindFirstChild("HumanoidRootPart")
        if not targetHrp then
            showOutput("âŒ Cannot find HumanoidRootPart for " .. targetPlayer.Name)
            return
        end
        
        -- Teleport to a small distance from target
        local targetCFrame = targetHrp.CFrame + targetHrp.CFrame.LookVector * -8 + Vector3.new(0, 3, 0)
        
        if safeTeleportTo(targetCFrame) then
            showOutput("ðŸŽ¯ Teleported to player: " .. targetPlayer.Name)
        else
            showOutput("âŒ Teleport error to " .. targetPlayer.Name)
        end
    end
    
    -- Create ESP for player
    local function createESP(player)
        if player == plr then return end -- Don't show ourselves
        
        local character = player.Character
        if not character then return end
        
        local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if not humanoidRootPart or not humanoid then return end
        
        -- Create folder for ESP elements of this player
        local espFolder = Instance.new("Folder")
        espFolder.Name = player.Name
        espFolder.Parent = ESPGui
        espFolders[player] = espFolder
        
        -- Create BillboardGui for display above head
        local billboard = Instance.new("BillboardGui")
        billboard.Name = "ESP"
        billboard.Size = UDim2.new(0, 100, 0, 25)
        billboard.ExtentsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true
        billboard.Enabled = true
        billboard.Active = true 
        billboard.Parent = espFolder
        
        -- Attach to player's head
        local head = character:FindFirstChild("Head")
        if head then
            billboard.Adornee = head
        else
            billboard.Adornee = humanoidRootPart
        end
        
        -- Background for name
        local nameBackground = Instance.new("Frame")
        nameBackground.Name = "Background"
        nameBackground.Size = UDim2.new(1, 0, 0, 25)
        nameBackground.Position = UDim2.new(0, 0, 0, 0)
        nameBackground.BackgroundColor3 = Color3.new(0, 0, 0)
        nameBackground.BackgroundTransparency = 0.3
        nameBackground.BorderSizePixel = 0
        nameBackground.Parent = billboard
        
        -- Background rounding
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 8)
        corner.Parent = nameBackground
        
        -- Border
        local stroke = Instance.new("UIStroke")
        stroke.Color = Color3.new(1, 1, 1)
        stroke.Thickness = 1
        stroke.Transparency = 0.5
        stroke.Parent = nameBackground
        
        -- Player name text (CLICKABLE)
        local nameLabel = Instance.new("TextButton")
        nameLabel.Name = "NameLabel"
        nameLabel.Size = UDim2.new(1, 0, 1, 0)
        nameLabel.Position = UDim2.new(0, 0, 0, 0)
        nameLabel.BackgroundTransparency = 1
        nameLabel.Text = player.Name
        nameLabel.TextColor3 = Color3.new(1, 1, 1)
        nameLabel.TextSize = 14
        nameLabel.Font = Enum.Font.GothamBold
        nameLabel.TextWrapped = true
        nameLabel.AutoButtonColor = false 
        nameLabel.Active = true 
        nameLabel.Parent = nameBackground
        
        -- Distance under name
        local distanceLabel = Instance.new("TextLabel")
        distanceLabel.Name = "DistanceLabel"
        distanceLabel.Size = UDim2.new(1, 0, 0, 15)
        distanceLabel.Position = UDim2.new(0, 0, 0, 25)
        distanceLabel.BackgroundTransparency = 1
        distanceLabel.Text = "0 studs"
        distanceLabel.TextColor3 = Color3.new(0.8, 0.8, 0.8)
        distanceLabel.TextSize = 10
        distanceLabel.Font = Enum.Font.Gotham
        distanceLabel.Parent = billboard
        
        -- CLICK HANDLER FOR TELEPORTATION
        nameLabel.MouseButton1Click:Connect(function()
            showOutput("ðŸ”„ Teleporting to " .. player.Name .. "...")
            teleportToPlayer(player)
        end)
        
        -- Hover animation (visual feedback)
        nameLabel.MouseEnter:Connect(function()
            local tween = TweenService:Create(nameBackground, TweenInfo.new(0.2), {
                BackgroundTransparency = 0.1,
                BackgroundColor3 = Color3.new(0.1, 0.3, 0.1) -- Green on hover
            })
            tween:Play()
            
            local textTween = TweenService:Create(nameLabel, TweenInfo.new(0.2), {
                TextColor3 = Color3.new(0, 1, 0) -- Bright green text
            })
            textTween:Play()
        end)
        
        nameLabel.MouseLeave:Connect(function()
            local tween = TweenService:Create(nameBackground, TweenInfo.new(0.2), {
                BackgroundTransparency = 0.3,
                BackgroundColor3 = Color3.new(0, 0, 0) -- Return to black
            })
            tween:Play()
            
            local textTween = TweenService:Create(nameLabel, TweenInfo.new(0.2), {
                TextColor3 = Color3.new(1, 1, 1) -- White text
            })
            textTween:Play()
        end)
        
        -- Distance update function
        local function updateDistance()
            if not espEnabled then return end
            
            local localChar = plr.Character
            local localHrp = localChar and localChar:FindFirstChild("HumanoidRootPart")
            local targetHrp = character:FindFirstChild("HumanoidRootPart")
            
            if localHrp and targetHrp and humanoid.Health > 0 then
                local distance = (localHrp.Position - targetHrp.Position).Magnitude
                distanceLabel.Text = string.format("%.0f studs", distance)
                
                -- Change color based on distance
                if distance < 20 then
                    nameLabel.TextColor3 = Color3.new(1, 0.2, 0.2) -- Red (close)
                elseif distance < 50 then
                    nameLabel.TextColor3 = Color3.new(1, 0.8, 0.2) -- Orange
                else
                    nameLabel.TextColor3 = Color3.new(0.2, 1, 0.2) -- Green (far)
                end
            else
                distanceLabel.Text = "N/A"
            end
        end
        
        -- Start distance update
        local distanceConnection = RunService.Heartbeat:Connect(updateDistance)
        table.insert(espConnections, distanceConnection)

        local scale = math.clamp(1 / (distance / 30), 0.33, 1)
        billboard.Size = UDim2.new(0, 200 * scale, 0, 50 * scale)
    end
    
    -- Remove player ESP function
    local function removeESP(player)
        if espFolders[player] then
            espFolders[player]:Destroy()
            espFolders[player] = nil
        end
    end
    
    -- Enable ESP function
    local function enableESP()
        if espEnabled then 
            showOutput("ðŸ‘ï¸ ESP: already enabled")
            return 
        end
        
        espEnabled = true
        showOutput("ðŸ‘ï¸ ESP: enabled - click on player names to teleport!")
        
        -- Create ESP for all existing players
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= plr then
                pcall(createESP, player)
            end
        end
        
        -- Handler for new players
        local playerAddedConnection = Players.PlayerAdded:Connect(function(player)
            if espEnabled then
                pcall(function() createESP(player) end)
            end
        end)
        table.insert(espConnections, playerAddedConnection)
        
        -- Handler for leaving players
        local playerRemovingConnection = Players.PlayerRemoving:Connect(function(player)
            pcall(function() removeESP(player) end)
        end)
        table.insert(espConnections, playerRemovingConnection)
        
        -- Handler for character respawn
        local characterAddedConnection = plr.CharacterAdded:Connect(function()
            -- Recreate ESP on respawn
            if espEnabled then
                for _, player in ipairs(Players:GetPlayers()) do
                    if player ~= plr then
                        pcall(function() removeESP(player) end)
                        pcall(function() createESP(player) end)
                    end
                end
            end
        end)
        table.insert(espConnections, characterAddedConnection)
    end
    
    -- Disable ESP function
    local function disableESP()
        if not espEnabled then 
            showOutput("ðŸ‘ï¸ ESP: already disabled")
            return 
        end
        
        espEnabled = false
        showOutput("ðŸ‘ï¸ ESP: disabled")
        
        -- Disconnect all connections
        for _, connection in ipairs(espConnections) do
            if connection then
                pcall(function() connection:Disconnect() end)
            end
        end
        espConnections = {}
        
        -- Remove all ESP elements
        for player, folder in pairs(espFolders) do
            if folder then
                pcall(function() folder:Destroy() end)
            end
        end
        espFolders = {}
    end
    
    -- Toggle ESP function
    local function toggleESP()
        if espEnabled then
            disableESP()
        else
            enableESP()
        end
    end
    
    -- Get player list function
    local function getPlayerList()
        local players = Players:GetPlayers()
        local playerList = "ðŸ‘¥ Players online (" .. #players .. "):\n"
        
        for _, player in ipairs(players) do
            if player == plr then
                playerList = playerList .. "â€¢ " .. player.Name .. " (You)\n"
            else
                playerList = playerList .. "â€¢ " .. player.Name .. "\n"
            end
        end
        
        return playerList
    end
    
    return {
        enable = enableESP,
        disable = disableESP,
        toggle = toggleESP,
        getPlayerList = getPlayerList,
        isEnabled = function() return espEnabled end
    }
end

