        if command == "help" then
            outputFrame.Size = UDim2.new(0, 350, 0, 900)
            showOutput("ðŸ“š AVAILABLE COMMANDS:\n\nðŸ“Š Main:\nstats - open statistics\nts - start tracking\ntss - stop tracking\ntsg - open progress panel\nclear - clear output\n\nðŸ‘Š FS Autofarm:\nfs on - enable FS autofarm\nfs off - disable FS autofarm\nrock - teleport to Rock\ncrystal - teleport to Crystal\nbluegod - teleport to Blue God Star\ngreengod - teleport to Green God Star\nredgod - teleport to Red God Star\n\nðŸ›¡ï¸ BT Autotrain:\nbt on - enable BT autotrain\nbt off - disable BT autotrain\n\nâš¡ Auto-systems:\nautotp on - enable auto-return\nautotp off - disable auto-return\n\nðŸ‘ï¸ ESP commands:\nesp on - enable ESP\nesp off - disable ESP\nesp toggle - toggle ESP\nplayers - player list\ntp @Name - teleport to player")

         -- ESP commands
        elseif command == "esp on" then
            ESPSystem.enable()
            showOutput("ðŸ‘ï¸ ESP: enabled")
        
        elseif command == "esp off" then
            ESPSystem.disable()
            showOutput("ðŸ‘ï¸ ESP: disabled")
        
        elseif command == "esp toggle" then
            ESPSystem.toggle()
        
        elseif command == "players" then
            outputFrame.Size = UDim2.new(0, 350, 0, 900)
            showOutput(ESPSystem.getPlayerList())
        
        elseif string.sub(command, 1, 4) == "tp @" then
            local playerName = string.sub(command, 5)
            if playerName and playerName ~= "" then
                ESPSystem.teleportToPlayerByName(playerName)
            else
                showOutput("âŒ Specify player name: tp @PlayerName")
            end

        -- Main commands
        elseif command == "stats" then
            statsPanel.toggle()
            showOutput("ðŸ“Š Opening statistics...")
        
        elseif command == "ts" then
            progressPanel.startTracking()
            showOutput("ðŸ“ˆ Starting progress tracking")
        
        elseif command == "tss" then
            progressPanel.stopTracking()
            showOutput("ðŸ“‰ Stopping tracking")
        
        elseif command == "tsg" then
            progressPanel.toggle()
            showOutput("ðŸ“Š Opening progress panel")
        
        elseif command == "clear" then
            outputLabel.Text = ""
            showOutput("ðŸ§¹ Output cleared")
            
        elseif command == "wiki" then
            wikiSystem.toggle()
            showOutput("ðŸ“š Wiki: " .. (wikiSystem.gui.MainFrame.Visible and "opened" or "closed"))

        -- FS commands
        elseif command == "fs on" then
            startFS()
        
        elseif command == "fs off" then
            stopFS()

        -- FS teleports
        elseif FS_LOCATIONS[command] then
            teleportTo(FS_LOCATIONS[command])
            showOutput("ðŸ”„ Teleporting: " .. command)

        -- BT commands
        elseif command == "bt on" then
            startBT()
        
        elseif command == "bt off" then
            stopBT()

        -- AUTOTP commands
        elseif command == "autotp on" then
            startAutotp()
        
        elseif command == "autotp off" then
            stopAutotp()
        
        else
            showOutput("âŒ Unknown command: " .. command .. "\nType 'help' for command list.")
        end
        
        -- Add to history
        table.insert(history, message)
        historyIndex = #history + 1
    end

    -- Input handlers
    TextButton.MouseButton1Click:Connect(function()
        local message = TextBox.Text
        if message ~= "" then
            handleCommand(message)
            TextBox.Text = ""
        end
    end)

    TextBox.FocusLost:Connect(function(enterPressed)
        if enterPressed then
            local message = TextBox.Text
            if message ~= "" then
                handleCommand(message)
            end
            TextBox.Text = ""
        end
    end)

    -- Hotkeys
    UIS.InputBegan:Connect(function(input, processed)
        if processed then return end
        
        if input.KeyCode == Enum.KeyCode.F2 then
            toggleInterface()
            if isExpanded then
                TextBox:CaptureFocus()
            end
        elseif input.KeyCode == Enum.KeyCode.Escape then
            if isExpanded then
                toggleInterface()
            end
        end
    end)

    return consoleGui
end


